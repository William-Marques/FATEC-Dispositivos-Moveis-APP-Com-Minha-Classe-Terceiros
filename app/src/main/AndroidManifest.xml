            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity
            android:name=".telas.usuario.ListUsuActivity"
            android:label="@string/title_activity_list_usu"
            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity
            android:name=".telas.usuario.AddUsuActivity"
            android:label="@string/title_activity_add_usu"
            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity android:name=".telas.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>