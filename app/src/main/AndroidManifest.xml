<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fatecmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FatecMobile">
        <activity
            android:name=".telas.terceiro.UptTerceiroActivity"
            android:label="@string/title_activity_upt_terceiro"
            android:theme="@style/Theme.FatecMobile.NoActionBar"></activity>
        <activity
            android:name=".telas.terceiro.ListTerceiroParamActivity"
            android:label="@string/title_activity_list_terceiro_param"
            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity
            android:name=".telas.terceiro.ListTerceiroActivity"
            android:label="@string/title_activity_list_terceiro"
            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity
            android:name=".telas.terceiro.AddTerceiroActivity"
            android:label="@string/title_activity_add_terceiro"
            android:theme="@style/Theme.FatecMobile.NoActionBar" />
        <activity
            android:name=".telas.usuario.UptUsuActivity"
